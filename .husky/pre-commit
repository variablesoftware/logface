#eval "$(nodenv init - zsh)"
source ~/.nvm/nvm.sh
set -x

nvm use 22 --lts
corepack enable && corepack prepare yarn@4.9.1 --activate
echo $PATH
which yarn node git
#export NPM_TOKEN="$(security find-generic-password -a $USER -s SECRET_NPM_TOKEN -w 2>/dev/null)"
yarn lint || exit 1
echo "Linting passed"